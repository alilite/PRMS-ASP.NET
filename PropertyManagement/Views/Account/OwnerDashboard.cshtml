<h2>Owner Dashboard</h2>
<div class="card mb-4">
    <div class="card-header">
        <h3>Account Details</h3>
    </div>
    <div class="card-body">
        <p><strong>ID:</strong> @Model.OwnerId</p>
        <p><strong>Name:</strong> @Model.Name</p>
        <p><strong>Email:</strong> @Model.Email</p>
        <p><strong>Phone:</strong> @Model.Phone</p>
    </div>
</div>
<ul>
    <li><a href="@Url.Action("ManageTenants", "Owner")">Manage Tenants</a></li>
    <li><a href="@Url.Action("ManagePropertyManagers", "Owner")">Manage Property Managers</a></li>
</ul>
